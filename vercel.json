{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "functions": {
      "api/**.js": {
        "maxDuration": 10,
        "memory": 512,
        "headers": {
          "Access-Control-Allow-Origin": "https://choronocam.vercel.app",
          "Access-Control-Allow-Methods": "GET, POST, PUT",
          "Access-Control-Allow-Headers": "Content-Type, Authorization"
        }
      }
    }
  }
  